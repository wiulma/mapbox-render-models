{
  "name": "mapboxrendermodels",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "compile": "tsc -p .",
    "ios": "react-native run-ios",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --fix",
    "start": "react-native start",
    "reset-cache": "rm -rf $TMPDIR/react-*; rm -rf $TMPDIR/haste-*; rm -rf $TMPDIR/metro-* && watchman watch-del-all",
    "start:reset-cache": "npm run reset-cache && react-native start --reset-cache",
    "clean:android": "rm -rf ./andriod/app/build && cd ./android && ./gradlew clean && cd ..",
    "clean:ios": "rm -rf ./ios/build",
    "clean": "rm -rf ios/Pods && npm run clean:android && npm run clean:ios && rm -rf node_modules",
    "reinstall:android": "npm run reset-cache && npm run clean:android && npm i --force",
    "reinstall:ios": "npm run reset-cache && npm run clean:ios && npm i --force && cd ios && pod install && cd ..",
    "reinstall": "npm run reset-cache && npm run clean && npm i --force && cd ios && pod install && cd ..",
    "test": "jest"
  },
  "dependencies": {
    "@rnmapbox/maps": "github:rnmapbox/maps#main",
    "react": "18.2.0",
    "react-native": "0.71.1"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-test-renderer": "^18.0.0",
    "babel-jest": "^29.2.1",
    "eslint": "^8.19.0",
    "jest": "^29.2.1",
    "metro-react-native-babel-preset": "0.73.7",
    "prettier": "^2.4.1",
    "react-native-dotenv": "^3.4.7",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./__tests__/mock/NativeEventEmitter_mock.js"
    ]
  }
}